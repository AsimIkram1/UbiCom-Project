<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_battery_test"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.asimikram.batterytest_new.BatteryTest">

    <TextView
        android:text="Percentage"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:id="@+id/perView"
        android:layout_alignBaseline="@+id/perLabel"
        android:layout_alignBottom="@+id/perLabel"
        android:layout_alignLeft="@+id/batteryView"
        android:layout_alignStart="@+id/batteryView"
        android:layout_x="141dp"
        android:layout_y="65dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/statusLabel"
        android:layout_toRightOf="@+id/statusLabel"
        android:layout_toEndOf="@+id/statusLabel"
        android:layout_marginLeft="55dp"
        android:layout_marginStart="55dp"
        android:id="@+id/batteryView"
        android:text="Charging?"
        android:layout_row="2"
        android:layout_column="0"
        android:layout_x="186dp"
        android:layout_y="50dp" />

    <View style="@style/Divider2"
        android:foreground="@color/common_google_signin_btn_text_light_focused"
        android:layout_height="2dp"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView6"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:clickable="false"
        android:editable="false" />

    <TextView
        android:text="Longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7"
        android:layout_marginTop="16dp"
        android:id="@+id/textView11"
        android:clickable="false"
        android:editable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignRight="@+id/newLat"
        android:layout_alignEnd="@+id/newLat"
        android:id="@+id/newLong"
        android:text="0.0" />

    <TextView
        android:text="Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/newLat"
        android:layout_alignLeft="@+id/inLocation"
        android:layout_alignStart="@+id/inLocation"
        android:layout_marginBottom="12dp"
        android:id="@+id/outLocation" />

    <Button
        android:text="Show Running Apps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/appsButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/oldLong"
        android:text="0.0"
        android:layout_above="@+id/textView6"
        android:layout_alignLeft="@+id/oldLat"
        android:layout_alignStart="@+id/oldLat" />

    <TextView
        android:text="Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="18dp"
        android:id="@+id/inLocation"
        android:editable="false"
        android:layout_above="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignEnd="@+id/textView4" />

    <TextView
        android:text="Longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/textView5"
        android:layout_x="165dp"
        android:layout_y="69dp"
        android:clickable="false"
        android:editable="false"
        android:layout_below="@+id/oldLat"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignEnd="@+id/textView4" />

    <TextView
        android:text="Latitude"
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="218dp"
        android:layout_y="53dp"
        android:editable="false"
        android:clickable="false"
        android:layout_above="@+id/textView5"
        android:layout_alignRight="@+id/perLabel"
        android:layout_alignEnd="@+id/perLabel" />

    <Button
        android:text="Battery Usage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/levelShow"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/oldLong"
        android:layout_alignStart="@+id/oldLong"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/oldLat"
        android:text="0.0"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/newLat"
        android:text="0.0"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignLeft="@+id/levelShow"
        android:layout_alignStart="@+id/levelShow" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/outLocationName"
        android:text="                                    "
        android:layout_alignBaseline="@+id/outLocation"
        android:layout_alignBottom="@+id/outLocation"
        android:layout_alignLeft="@+id/newLat"
        android:layout_alignStart="@+id/newLat" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/inLocationName"
        android:text="                                 "
        android:layout_alignBaseline="@+id/inLocation"
        android:layout_alignBottom="@+id/inLocation"
        android:layout_alignLeft="@+id/oldLat"
        android:layout_alignStart="@+id/oldLat" />

    <View style="@style/Divider"
        android:foreground="@color/common_google_signin_btn_text_light_focused"
        android:layout_height="2dp"
        android:layout_row="4"
        android:layout_column="0"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Plugged In"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:clickable="false"
        android:cursorVisible="false"
        android:editable="false"
        android:textSize="18sp"
        android:gravity="center"
        android:layout_row="2"
        android:layout_column="1"
        android:layout_x="16dp"
        android:layout_y="82dp"
        android:layout_above="@+id/inLocation"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Plugged Out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:id="@+id/textView6"
        android:gravity="center"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Percentage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/perLabel"
        android:layout_x="174dp"
        android:layout_y="29dp"
        android:layout_above="@+id/textView2"
        android:layout_alignLeft="@+id/statusLabel"
        android:layout_alignStart="@+id/statusLabel"
        android:layout_marginBottom="18dp" />

    <TextView
        android:text="Status"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:id="@+id/statusLabel"
        android:layout_x="93dp"
        android:layout_y="88dp"
        android:layout_above="@+id/perLabel"
        android:layout_alignRight="@+id/appsButton"
        android:layout_alignEnd="@+id/appsButton"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="13dp" />


</RelativeLayout>
